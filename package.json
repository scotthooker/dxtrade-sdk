{
  "name": "dxtrade-sdk",
  "version": "1.1.0",
  "description": "Production-ready TypeScript SDK for DXtrade REST and WebSocket APIs",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./rest": {
      "import": "./dist/rest/index.js",
      "types": "./dist/rest/index.d.ts"
    },
    "./websocket": {
      "import": "./dist/websocket/index.js",
      "types": "./dist/websocket/index.d.ts"
    }
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "tsx --watch src/index.ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "format:check": "prettier --check src/**/*.{ts,tsx}",
    "docs": "typedoc src/index.ts",
    "docs:serve": "typedoc src/index.ts --watch",
    "prepublishOnly": "npm run lint && npm run build",
    "example:config": "tsx examples/test-env-config.ts",
    "example:discover": "tsx examples/discover-endpoints.ts",
    "example:data-reception": "tsx examples/test-data-reception.ts",
    "example:dxtrade-stream": "tsx examples/dxtrade-stream-example.ts",
    "example:stream-test": "tsx examples/test-dxtrade-stream.ts",
    "example:websocket-5min": "tsx examples/test-websocket-5min.ts"
  },
  "keywords": [
    "dxtrade",
    "trading",
    "websocket",
    "rest-api",
    "typescript",
    "sdk",
    "financial",
    "market-data",
    "real-time",
    "api-client"
  ],
  "author": "Scott Hooker <scott@scotthooker.dev>",
  "repository": {
    "type": "git",
    "url": "https://github.com/scotthooker/dxtrade-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/scotthooker/dxtrade-sdk/issues"
  },
  "homepage": "https://github.com/scotthooker/dxtrade-sdk#readme",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^17.2.1",
    "ws": "^8.18.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "tsx": "^4.20.3",
    "typedoc": "^0.25.8",
    "typescript": "^5.3.3",
    "vite": "^7.0.6",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
